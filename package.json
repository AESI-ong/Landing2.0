{
  "name": "bun-react-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "src/index.tsx",
  "module": "src/index.tsx",
  "scripts": {
    "dev": "bun --hot src/index.tsx",
    "dev:css": "tailwindcss --input ./src/index.css --output ./src/output.css --watch",
    "build:css": "tailwindcss --input ./src/index.css --output ./src/output.css --minify",
    "build": "bun run build:css && bun build ./src/index.html --outdir=dist --sourcemap --target=browser --minify --define:process.env.NODE_ENV='\"production\"' --env='BUN_PUBLIC_*'",
    "start": "NODE_ENV=production bun src/index.tsx"
  },
  "dependencies": {
    "lucide-react": "^0.525.0",
    "react": "^19",
    "react-dom": "^19",
    "react-router-dom": "^7.6.3"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.11",
    "@types/bun": "latest",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "tailwindcss": "^4.1.11"
  }
}
